package kodlama.io.rentACar.entities.concretes;

import javax.persistence.Entity;
import javax.persistence.Table;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Table(name="brands")
//@Data    //getters ve settersleri temsil eder.
@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor
@Entity  //Veri taban覺 tablolar覺n覺 tutan veriler sendee

public class Brand {
	private int id;
	private String name;
	
//lombak avantaj覺
	

}
